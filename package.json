{
  "name": "react-wrangler",
  "version": "0.0.3",
  "description": "A React component for simple and declarative state management.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config=./webpack.config.dist.js && cp dist/build/index* .",
    "example": "webpack-dev-server --hot --inline --config=./webpack.config.examples.js",
    "prepublish": "npm test && rm -rf ./dist && npm run example && npm run build",
    "lint": "eslint src/** examples/**",
    "test": "npm run lint && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limscoder/react-wrangler.git"
  },
  "keywords": [
    "react",
    "falcor",
    "rest",
    "state"
  ],
  "author": "Dave Thompson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limscoder/react-wrangler/issues"
  },
  "homepage": "https://github.com/limscoder/react-wrangler#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "enzyme": "^2.7.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "file-loader": "0.9.0",
    "jest": "18.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "1.3.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "peerDependencies": {
    "immutable": "^3.8.0",
    "react": "^15.0.0"
  },
  "dependencies": {
    "react-immutable-proptypes": "^2.1.0",
    "shallow-equals": "0.0.0"
  }
}
